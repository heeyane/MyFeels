\";\nimport Learn from \"../../pages/learn\";\nimport Oops from \"../../pages/oops\";\n\nfunction App() {\n    // current user\n    const [user, setUser] = useState(null);\n\n    // handle user login\n    const handleUser = (userData) => {\n        let currentUser = userData.data;\n        setUser(currentUser);\n    };\n\n    // handle user logout\n    const handleLogout = () => {\n        setUser(null);\n    };\n\n    // current user entry data\n    const [userEntries, setUserEntries] = useState(null);\n    useEffect(() => {\n        if (user) {\n            getEntries();\n        }\n    }, [user]);\n\n    // get entries from database\n    const getEntries = () => {\n        API.getEntries(user)\n            .then((res) => {\n                setUserEntries(res.data);\n            })\n            .catch((err) => {\n                console.log(\"error: \", err);\n            });\n    };\n\n    // submit entry to database\n    const submitEntry = (entryData) => {\n        API.createEntry(user, entryData)\n            .then((res) => {\n                getEntries();\n            })\n            .catch((err) => {\n                console.log(\"error: \", err);\n            });\n    };\n\n    return (\n        <div className=\"mobile\">\n            <Router>\n                {!user ? (\n                    <Route path=\"/\">\n                        <Redirect to=\"/home\" />\n                        <Login authUser={handleUser} />\n                    </Route>\n                ) : (\n                    <>\n                        <Navbar logoutUser={handleLogout} />\n\n                        <Switch>\n                            <Route exact path={[\"/\", \"/hows-your-spirit\", \"/home\"]}>\n                                <Home entries={userEntries} />\n                            </Route>\n                            <Route path=\"/add\">\n                                <Add submitEntry={submitEntry} />\n                            </Route>\n                            <Route path=\"/review\">\n                                <Review entries={userEntries} />\n                            </Route>\n                            <Route path=\"/learn\">\n                                <Learn />\n                            </Route>\n                            <Route>\n                                <Oops />\n                            </Route>\n                        </Switch>\n                    </>\n                )}\n            </Router>\n        </div>\n    );\n}\n\nexport default App;","/Users/HeeSeung/Desktop/project4/my-feels/client/src/pages/review.js",[],"/Users/HeeSeung/Desktop/project4/my-feels/client/src/pages/add.js",["109"],"import React, { useState, useEffect } from \"react\";\nimport { Link } from \"react-router-dom\";\nimport { Button } from \"react-bootstrap\";\nimport moment from \"moment\";\n\nimport \"../components/App/app.css\";\nimport EmotionMap from \"../utils/EmotionMap\";\nimport EnterEmotion from \"../components/EnterEmotion\";\nimport EnterActivity from \"../components/EnterActivity\";\n\n\nfunction Add({ submitEntry }) {\n    // entry submission status\n    const [entryStatus, setEntryStatus] = useState(\"addEmotion\");\n    // entry data\n    const [entryData, setEntryData] = useState({ date: moment().format(\"MMMM Do YYYY\"), emotions: [], factors: [] });\n    useEffect(() => {\n        if (entryStatus === \"complete\") {\n            submitEntry(entryData);\n        }\n    }, [entryData]);\n\n    // handle submit buttons\n    const handleSubmit = (data, submitType) => {\n        switch (submitType) {\n            case \"addEmotion\":\n            case \"completeEmotion\":\n                if (data.feeling && data.weight) {\n                    let feelingEntry = EmotionMap.feeling(data.feeling);\n                    feelingEntry.weight = data.weight;\n                    setEntryData({ ...entryData, emotions: [...entryData.emotions, feelingEntry] });\n                }\n                break;\n            case \"complete\":\n                setEntryData({ ...entryData, factors: data });\n                break;\n            default:\n                console.log(\"oops: \", submitType);\n        }\n        setEntryStatus(submitType);\n    };\n\n    return (\n        <main style={{ backgroundColor: \"#BFE2FF\" }}>\n            <div className=\"flexbox-container\" style={{ backgroundColor: \"white\" }}>\n                <div className=\"container\" id=\"entryDiv\"></div>\n                {(() => {\n                    switch (entryStatus) {\n                        case \"addEmotion\":\n                            return (\n                                <div>\n                                    <EnterEmotion submitEmotion={handleSubmit} />\n                                </div>\n                            );\n                        case \"completeEmotion\":\n                            return (\n                                <div>\n                                    <EnterActivity submitActivity={handleSubmit} />\n                                </div>\n                            );\n                        case \"complete\":\n                            return (\n                                <div className=\"container\">\n                                    <h3 style={{ color: \"#FFC300\", textAlign: \"center\" }}>You've successfully submitted a mood entry.</h3>\n                                    <br />\n                                    <h3 style={{ color: \"#BFE2FF\", textAlign: \"center\" }}>Take a look at your mood trend.</h3>\n                                    <br />\n                                    <Button outline color=\"primary\" size=\"lg\" block as={Link} to=\"/review\">\n                                        Review your entries\n                                    </Button>\n                                </div>\n                            );\n                        default:\n                            return <div>Oops!</div>;\n                    }\n                })()}\n            </div>\n        </main>\n    );\n}\n\nexport default Add;","/Users/HeeSeung/Desktop/project4/my-feels/client/src/pages/home.js",[],"/Users/HeeSeung/Desktop/project4/my-feels/client/src/pages/login.js",[],"/Users/HeeSeung/Desktop/project4/my-feels/client/src/pages/oops.js",[],"/Users/HeeSeung/Desktop/project4/my-feels/client/src/pages/learn.js",[],"/Users/HeeSeung/Desktop/project4/my-feels/client/src/utils/API.js",["110"],"import axios from \"axios\";\n\nexport default {\n    //existing user login\n\n    login: (userData) =>\n        axios.get(\"/auth/\", {\n            params: {\n                email: userData.email,\n                password: userData.password,\n            },\n        }),\n\n    //new user signup\n    signup: (userData) => axios.post(\"/auth/\", userData),\n\n    //get entries by criteria\n    createEntry: (user, entryData) => axios.post(\"/entries\", { user: user, entry: entryData }),\n\n    getEntries: (id) => axios.get(\"/entries/\" + id),\n};","/Users/HeeSeung/Desktop/project4/my-feels/client/src/utils/EmotionMap.js",[],"/Users/HeeSeung/Desktop/project4/my-feels/client/src/components/Navbar/index.js",[],"/Users/HeeSeung/Desktop/project4/my-feels/client/src/components/Navbar/navbar.js",[],"/Users/HeeSeung/Desktop/project4/my-feels/client/src/components/User/index.js",[],"/Users/HeeSeung/Desktop/project4/my-feels/client/src/components/User/user.js",[],"/Users/HeeSeung/Desktop/project4/my-feels/client/src/components/EnterActivity/index.js",[],"/Users/HeeSeung/Desktop/project4/my-feels/client/src/components/EnterActivity/enteractivity.js",[],"/Users/HeeSeung/Desktop/project4/my-feels/client/src/components/EnterEmotion/index.js",[],"/Users/HeeSeung/Desktop/project4/my-feels/client/src/components/EnterEmotion/enteremotion.js",[],"/Users/HeeSeung/Desktop/project4/my-feels/client/src/components/ViewActivity/index.js",[],"/Users/HeeSeung/Desktop/project4/my-feels/client/src/component